{
    "name": "ajardin/blizzard-sdk-bundle",
    "description": "A Symfony bundle for Blizzard API.",
    "license": "MIT",
    "type": "symfony-bundle",
    "authors": [
        {
            "name": "Alexandre Jardin",
            "email": "info@ajardin.fr"
        }
    ],
    "require": {
        "php": ">=8.0",
        "symfony/framework-bundle": "^5.3 || ^6.0 || ^7.0",
        "symfony/http-client": "^5.3 || ^6.0 || ^7.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.13.2",
        "phpspec/prophecy": "^1.16",
        "phpspec/prophecy-phpunit": "^2.0.1",
        "phpstan/phpstan": "^1.9.7",
        "phpunit/phpunit": "^9.5.27",
        "rector/rector": "^0.15.2",
        "symfony/phpunit-bridge": "^6.2.3",
        "vimeo/psalm": "^5.4"
    },
    "autoload": {
        "psr-4": {
            "Ajardin\\BlizzardSdkBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ajardin\\BlizzardSdkBundle\\Tests\\": "tests/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    }
}
