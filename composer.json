{
    "name": "ajardin/blizzard-sdk-bundle",
    "description": "",
    "type": "symfony-bundle",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Ajardin\\BlizzardSdkBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ajardin\\BlizzardSdkBundle\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Alexandre Jardin",
            "email": "info@ajardin.fr"
        }
    ],
    "require": {
        "php": ">=8.0",
        "symfony/framework-bundle": "^5.3 || ^6.0",
        "symfony/http-client": "^5.3 || ^6.0"
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.8",
        "phpspec/prophecy": "^1.15",
        "phpspec/prophecy-phpunit": "^2.0",
        "phpstan/phpstan": "^1.7",
        "phpunit/phpunit": "^9.5",
        "rector/rector": "^0.13.5",
        "symfony/phpunit-bridge": "^6.0",
        "vimeo/psalm": "^4.23"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    }
}
